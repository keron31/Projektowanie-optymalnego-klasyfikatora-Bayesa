<Window x:Class="Projektowanie_optymalnego_klasyfikatora_Bayesa.CustomDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projektowanie_optymalnego_klasyfikatora_Bayesa"
        mc:Ignorable="d"
        Title="Wyniki" Height="600" Width="400"
        Icon="generate_logo_for_artificial_intelligence.png">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <RichTextBox x:Name="rtbResults" IsReadOnly="True" />
        <Button x:Name="btnClose" Grid.Row="1" Content="Zamknij" Click="btnClose_Click" Width="100" Height="30" HorizontalAlignment="Right" Margin="0,10"/>
    </Grid>
</Window>
